{
  "name": "alakrity",
  "version": "0.0.2",
  "repository": "https://bitbucket.org/philipp_s/alakrity2",
  "description": "A scheduling app for procrastinators.",
  "main": "index.js",
  "scripts": {
    "start-prod": "set NODE_ENV=production&& npm run db-setup&& set NODE_PATH=$NODE_PATH:./shared&& node --harmony .",
    "start": "set NODE_PATH=$NODE_PATH:./shared&& node --harmony .",
    "startall": "set NODE_ENV=development&& set NODE_PATH=$NODE_PATH:./shared&& node --harmony .",
    "start2": "node --harmony .",
    "dbtool": "set NODE_ENV=development&& chateau -f chateau.config.js",
    "dev": "set NODE_ENV=development&&set NODE_SOURCEMAP=eval&& npm run start & webpack-dev-server --progress --color",
    "devall": "npm-run-all -p dbtool startall & webpack-dev-server --progress --color",
    "build": "set NODE_ENV=production&& webpack --progress --color -p --config webpack.prod.config.js",
    "db-setup": "node dbSetup.babel.js"
  },
  "author": "Philipp",
  "license": "MIT",
